@model EXPEDIT.Transactions.ViewModels.OrderViewModel
<h2>Download Details:</h2>
<ul>
    <li><a href="/share/getorderinvoice/@Model.OrderID">Download Invoice</a></li>
    @foreach (var m in Model.Downloads) {
        <li><a href="/share/download/@m.DownloadID">Download @m.Description</a></li>
    }
</ul>